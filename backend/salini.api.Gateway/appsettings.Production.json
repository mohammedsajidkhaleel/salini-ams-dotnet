{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Yarp": "Information"
      }
    }
  },
  "ReverseProxy": {
    "Clusters": {
      "api-cluster": {
        "Destinations": {
          "api-destination": {
            "Address": "http://localhost:5000/"
          }
        }
      },
      "client-cluster": {
        "Destinations": {
          "client-destination": {
            "Address": "http://localhost:3000/"
          }
        }
      }
    }
  }
}
